{
  "name": "@dathomir/runtime",
  "description": "dathomir runtime package",
  "version": "0.0.1-beta.1",
  "private": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/",
    "provenance": true
  },
  "type": "module",
  "license": "MPL-2.0",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:dathomir-js/dathomir.git"
  },
  "bugs": {
    "url": "https://github.com/dathomir-js/dathomir/issues",
    "email": "dathomir@takumaru.dev"
  },
  "homepage": "https://github.com/dathomir-js/dathomir#readme",
  "author": "takuma-ru <takuma-ru@takumaru.dev> (https://github.com/takuma-ru/)",
  "keywords": [
    "dathomir",
    "framework",
    "frontend",
    "javascript",
    "typescript"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./ssr": {
      "import": {
        "types": "./dist/ssr/index.d.mts",
        "default": "./dist/ssr/index.mjs"
      },
      "require": {
        "types": "./dist/ssr/index.d.cts",
        "default": "./dist/ssr/index.cjs"
      }
    },
    "./hydration": {
      "import": {
        "types": "./dist/hydration/index.d.mts",
        "default": "./dist/hydration/index.mjs"
      },
      "require": {
        "types": "./dist/hydration/index.d.cts",
        "default": "./dist/hydration/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsdown --watch",
    "build": "tsdown",
    "typecheck": "tsc --noEmit",
    "lint": "oxlint && eslint .",
    "test": "vitest run --coverage",
    "bench": "vitest bench --run --config vitest.bench.config.ts",
    "release": "rlse"
  },
  "dependencies": {
    "@dathomir/reactivity": "workspace:*",
    "@dathomir/shared": "workspace:*",
    "devalue": "^5.6.2"
  },
  "devDependencies": {
    "@dathomir/config": "workspace:*",
    "@takuma-ru/rlse": "catalog:",
    "@types/node": "24.2.0",
    "@vitest/browser-playwright": "catalog:",
    "@vitest/coverage-v8": "catalog:",
    "happy-dom": "^20.5.3",
    "playwright": "^1.56.1",
    "vitest": "catalog:"
  }
}
