{
  "name": "@dathomir/core",
  "description": "dathomir package",
  "version": "0.0.0",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "license": "MPL-2.0",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:dathomir-js/dathomir.git"
  },
  "bugs": {
    "url": "https://github.com/dathomir-js/dathomir/issues",
    "email": "dathomir@takumaru.dev"
  },
  "homepage": "https://github.com/dathomir-js/dathomir#readme",
  "author": "takuma-ru <takuma-ru@takumaru.dev> (https://github.com/takuma-ru/)",
  "keywords": [
    "dathomir",
    "framework",
    "frontend",
    "javascript",
    "typescript"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.mts"
    },
    "./runtime": {
      "import": "./dist/runtime/index.mjs",
      "require": "./dist/runtime/index.cjs",
      "types": "./dist/runtime/index.d.mts"
    },
    "./runtime/jsx-runtime": {
      "import": "./dist/runtime/jsx-runtime.mjs",
      "require": "./dist/runtime/jsx-runtime.cjs",
      "types": "./dist/runtime/jsx-runtime.d.mts"
    },
    "./runtime/jsx-dev-runtime": {
      "import": "./dist/runtime/jsx-dev-runtime.mjs",
      "require": "./dist/runtime/jsx-dev-runtime.cjs",
      "types": "./dist/runtime/jsx-dev-runtime.d.mts"
    },
    "./reactivity": {
      "import": "./dist/reactivity/index.mjs",
      "require": "./dist/reactivity/index.cjs",
      "types": "./dist/reactivity/index.d.mts"
    },
    "./shared": {
      "import": "./dist/shared/index.mjs",
      "require": "./dist/shared/index.cjs",
      "types": "./dist/shared/index.d.mts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsdown --watch",
    "build": "tsdown",
    "typecheck": "tsc --noEmit",
    "lint": "oxlint",
    "test": "vitest run --coverage",
    "version": "printf '\\033[1;36m%-10s | %-10s | %-10s\\033[0m\\n' tool version which; printf '%-10s | %-10s | %-10s\\n' ---------- ---------- ----------; printf '%-10s | %-10s | %-10s\\n' '@antfu/ni' '' \"$(which ni)\"; printf '%-10s | %-10s | %-10s\\n' node \"$(node -v)\" \"$(which node)\"; printf '%-10s | %-10s | %-10s\\n' pnpm \"v$(pnpm -v)\" \"$(which pnpm)\"",
    "release": "rlse"
  },
  "devDependencies": {
    "@dathomir/config": "workspace:*",
    "@takuma-ru/rlse": "catalog:"
  },
  "dependencies": {
    "@dathomir/reactivity": "workspace:*",
    "@dathomir/runtime": "workspace:*",
    "@dathomir/shared": "workspace:*"
  }
}
