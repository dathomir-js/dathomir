{
  "name": "@ailuros/core",
  "description": "Ailuros package",
  "version": "0.0.1-beta.1",
  "private": false,
  "type": "module",
  "license": "MPL-2.0",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:nucoui/ailuros.git"
  },
  "bugs": {
    "url": "https://github.com/nucoui/ailuros/issues",
    "email": "ailuros@takumaru.dev"
  },
  "homepage": "https://github.com/nucoui/ailuros#readme",
  "author": "takuma-ru <takuma-ru@takumaru.dev> (https://github.com/takuma-ru/)",
  "keywords": [
    "ailuros",
    "framework",
    "frontend",
    "javascript",
    "typescript"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./runtime/index": {
      "import": "./dist/runtime/index.js",
      "require": "./dist/runtime/index.cjs",
      "types": "./dist/runtime/index.d.ts"
    },
    "./runtime/jsx-runtime": {
      "import": "./dist/runtime/jsx-runtime.js",
      "require": "./dist/runtime/jsx-runtime.cjs",
      "types": "./dist/runtime/jsx-runtime.d.ts"
    },
    "./runtime/jsx-dev-runtime": {
      "import": "./dist/runtime/jsx-dev-runtime.js",
      "require": "./dist/runtime/jsx-dev-runtime.cjs",
      "types": "./dist/runtime/jsx-dev-runtime.d.ts"
    },
    "./reactivity": {
      "import": "./dist/reactivity/index.js",
      "require": "./dist/reactivity/index.cjs",
      "types": "./dist/reactivity/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsdown --watch",
    "build": "tsdown",
    "typecheck": "tsc --noEmit",
    "lint": "oxlint",
    "test": "vitest run --coverage"
  },
  "devDependencies": {
    "@ailuros/config": "workspace:*"
  },
  "dependencies": {
    "@ailuros/reactivity": "workspace:*",
    "@ailuros/runtime": "workspace:*"
  }
}
