{
  "name": "@dathomir/runtime",
  "description": "dathomir runtime package",
  "version": "0.0.0",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "license": "MPL-2.0",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:dathomir-js/dathomir.git"
  },
  "bugs": {
    "url": "https://github.com/dathomir-js/dathomir/issues",
    "email": "dathomir@takumaru.dev"
  },
  "homepage": "https://github.com/dathomir-js/dathomir#readme",
  "author": "takuma-ru <takuma-ru@takumaru.dev> (https://github.com/takuma-ru/)",
  "keywords": [
    "dathomir",
    "framework",
    "frontend",
    "javascript",
    "typescript"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./jsx-runtime": {
      "types": "./dist/jsx-runtime.d.mts",
      "import": "./dist/jsx-runtime.mjs",
      "require": "./dist/jsx-runtime.cjs"
    },
    "./jsx-dev-runtime": {
      "types": "./dist/jsx-dev-runtime.d.mts",
      "import": "./dist/jsx-dev-runtime.mjs",
      "require": "./dist/jsx-dev-runtime.cjs"
    },
    "./reactivity": {
      "types": "./dist/reactivity.d.mts",
      "import": "./dist/reactivity.mjs",
      "require": "./dist/reactivity.cjs"
    },
    "./ssr/renderToString": {
      "types": "./dist/ssr/renderToString.d.mts",
      "import": "./dist/ssr/renderToString.mjs",
      "require": "./dist/ssr/renderToString.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsdown --watch",
    "build": "tsdown",
    "typecheck": "tsc --noEmit",
    "lint": "oxlint && eslint .",
    "test": "vitest run --coverage",
    "bench": "vitest bench --run --config vitest.bench.config.ts",
    "release": "rlse"
  },
  "dependencies": {
    "@dathomir/reactivity": "workspace:*",
    "@dathomir/shared": "workspace:*"
  },
  "devDependencies": {
    "@dathomir/config": "workspace:*",
    "@takuma-ru/rlse": "catalog:",
    "@vitest/browser-playwright": "catalog:",
    "@vitest/coverage-v8": "catalog:",
    "playwright": "^1.56.1",
    "vitest": "catalog:"
  }
}
