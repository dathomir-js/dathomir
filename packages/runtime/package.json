{
  "name": "@dathomir/runtime",
  "description": "dathomir runtime package",
  "version": "0.0.1-beta.1",
  "private": false,
  "type": "module",
  "license": "MPL-2.0",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:dathomir-js/dathomir.git"
  },
  "bugs": {
    "url": "https://github.com/dathomir-js/dathomir/issues",
    "email": "dathomir@takumaru.dev"
  },
  "homepage": "https://github.com/dathomir-js/dathomir#readme",
  "author": "takuma-ru <takuma-ru@takumaru.dev> (https://github.com/takuma-ru/)",
  "keywords": [
    "dathomir",
    "framework",
    "frontend",
    "javascript",
    "typescript"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./jsx-runtime": {
      "types": "./dist/jsx-runtime.d.ts",
      "import": "./dist/jsx-runtime.js",
      "require": "./dist/jsx-runtime.cjs"
    },
    "./jsx-dev-runtime": {
      "types": "./dist/jsx-dev-runtime.d.ts",
      "import": "./dist/jsx-dev-runtime.js",
      "require": "./dist/jsx-dev-runtime.cjs"
    },
    "./reactivity": {
      "types": "./dist/reactivity.d.ts",
      "import": "./dist/reactivity.js",
      "require": "./dist/reactivity.cjs"
    },
    "./ssr/renderToString": {
      "types": "./dist/ssr/renderToString.d.ts",
      "import": "./dist/ssr/renderToString.js",
      "require": "./dist/ssr/renderToString.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsdown --watch",
    "build": "tsdown",
    "typecheck": "tsc --noEmit",
    "lint": "oxlint && eslint .",
    "test": "vitest run --coverage"
  },
  "dependencies": {
    "@dathomir/reactivity": "workspace:*",
    "@dathomir/shared": "workspace:*"
  },
  "devDependencies": {
    "@dathomir/config": "workspace:*",
    "@vitest/browser-playwright": "catalog:",
    "@vitest/coverage-v8": "catalog:",
    "playwright": "^1.56.1",
    "vitest": "catalog:"
  }
}
